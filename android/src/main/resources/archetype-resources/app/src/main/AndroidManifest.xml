<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="${package}">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera.front"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera.back"
        android:required="false" />

    <application
        tools:replace="android:allowBackup"
        android:name="CustomApplication"
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.adeuza.movalysfwk.mobile.mf4android.activity.ParameterDialogActivity"
            android:label="@string/activityname_parameterdialog"
            android:theme="@style/Theme.Dialog" >
        </activity>
        <activity
            android:name="com.adeuza.movalysfwk.mobile.mf4android.activity.ResetSettingsAndExitDialogActivity"
            android:label="@string/activityname_resetsettingsandexitdialog"
            android:theme="@style/Theme.Dialog" >
        </activity>
        <activity
            android:name="com.adeuza.movalysfwk.mobile.mf4android.activity.ParameterActivity"
            android:label="@string/activityname_parameter" >
            <intent-filter>
                <action android:name="com.adeuza.movalysfwk.mobile.mf4android.activity.business.displaysettings.ACTION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.adeuza.movalysfwk.mobile.mf4android.activity.DBImportValidationActivity"
            android:label="DBImport"
            android:theme="@style/Theme.Dialog" />
        <activity android:name="com.adeuza.movalysfwk.mobile.mf4android.ui.command.MMPhotoCommand" >
        </activity>
        <!--//@generated-start[gen-activities][X]-->
        <activity
            android:name="${package}.screens.defaultscreen.MainActivity"
            android:label="MainActivity" >
            <intent-filter>
                <action android:name="${package}.action.PROJECT_MAIN" />

                <category android:name="${package}.category.MF4A" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--//@generated-end-->
        <service android:name="com.adeuza.movalysfwk.mobile.mf4android.service.PushServiceImpl" />
    </application>

</manifest>